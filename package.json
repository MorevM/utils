{
	"name": "@morev/utils",
	"description": "Collection of JavaScript / TypeScript utilities",
	"type": "module",
	"version": "2.1.0",
	"private": false,
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"author": {
		"name": "Maxim Morev",
		"email": "max.seainside@gmail.com"
	},
	"homepage": "https://github.com/MorevM/utils#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/MorevM/utils"
	},
	"keywords": [
		"javascript",
		"typescript",
		"js",
		"ts",
		"utils",
		"utilities"
	],
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"prepare": "lefthook install",
		"vscode-settings": "cp .vscode/settings.template.json .vscode/settings.json",
		"build": "tsup",
		"test": "jest",
		"test:dev": "jest --watch",
		"lint": "eslint \"**/*.{js,ts,json,json5,jsonc,yml,yaml,md}\"",
		"lint:fix": "eslint \"**/*.{js,ts,json,json5,jsonc,yml,yaml,md}\" --fix",
		"release": "release-it --disable-metrics"
	},
	"dependencies": {
		"fast-copy": "^3.0.1",
		"fast-equals": "^5.0.1",
		"ohash": "^1.1.2",
		"type-fest": "^3.11.1"
	},
	"devDependencies": {
		"@morev/commitlint-config": "^0.1.1",
		"@morev/eslint-config": "^22.1.0",
		"@release-it/conventional-changelog": "^5.1.1",
		"@types/jest": "^29.5.1",
		"eslint": "^8.40.0",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"lefthook": "^1.3.12",
		"release-it": "^15.10.3",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"tsup": "^6.7.0",
		"typescript": "^5.0.4"
	}
}
