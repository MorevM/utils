{
	"name": "@morev/utils",
	"description": "Collection of JavaScript / TypeScript utilities",
	"type": "module",
	"version": "3.10.0",
	"private": false,
	"sideEffects": false,
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"author": {
		"name": "Maxim Morev",
		"email": "max.seainside@gmail.com"
	},
	"homepage": "https://github.com/MorevM/utils#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/MorevM/utils"
	},
	"keywords": [
		"javascript",
		"typescript",
		"js",
		"ts",
		"utils",
		"utilities"
	],
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"prepare": "lefthook install",
		"vscode-settings": "cp .vscode/settings.template.json .vscode/settings.json",
		"build": "rm -rf dist && tsup",
		"test": "vitest",
		"test:dev": "vitest --watch",
		"lint": "eslint \"**/*.{js,ts,json,json5,jsonc,yml,yaml,md}\"",
		"lint:fix": "eslint \"**/*.{js,ts,json,json5,jsonc,yml,yaml,md}\" --fix",
		"release": "release-it --disable-metrics"
	},
	"dependencies": {
		"fast-copy": "^3.0.2",
		"fast-equals": "^5.0.1",
		"ohash": "^1.1.3",
		"type-fest": "^4.18.3"
	},
	"devDependencies": {
		"@morev/commitlint-config": "^0.2.2",
		"@morev/eslint-config": "^32.0.0",
		"@release-it/conventional-changelog": "^8.0.1",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/node": "^20.12.7",
		"@vitest/coverage-v8": "^1.5.2",
		"eslint": "^8.57.0",
		"jest-date": "^1.1.6",
		"jest-environment-jsdom": "^29.7.0",
		"jsdom": "^24.0.0",
		"lefthook": "^1.6.10",
		"release-it": "^17.2.1",
		"ts-node": "^10.9.2",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5",
		"vitest": "^1.5.2"
	}
}
