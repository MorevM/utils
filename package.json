{
	"name": "@morev/utils",
	"description": "Collection of JavaScript / TypeScript utilities",
	"version": "2.0.0",
	"private": false,
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"author": {
		"name": "Maxim Morev",
		"email": "max.seainside@gmail.com"
	},
	"homepage": "https://github.com/MorevM/utils#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/MorevM/utils"
	},
	"keywords": [
		"javascript",
		"typescript",
		"js",
		"ts",
		"utils",
		"utilities"
	],
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"scripts": {
		"prepare": "husky install ./.husky",
		"vscode-settings": "cp .vscode/settings.template.json .vscode/settings.json",
		"build": "tsup",
		"test": "jest",
		"test:dev": "jest --watch",
		"lint": "eslint \"**/*.{js,ts,json,json5,jsonc,yml,yaml,md}\"",
		"lint:fix": "eslint \"**/*.{js,ts,json,json5,jsonc,yml,yaml,md}\" --fix",
		"release": "release-it --disable-metrics"
	},
	"dependencies": {
		"fast-copy": "^3.0.0",
		"fast-equals": "^4.0.3",
		"ohash": "^1.0.0",
		"type-fest": "^3.5.0"
	},
	"devDependencies": {
		"@morev/commitlint-config": "^0.1.1",
		"@morev/eslint-config": "^20.0.3",
		"@release-it/conventional-changelog": "^5.1.1",
		"@types/jest": "^29.2.5",
		"eslint": "^8.31.0",
		"husky": "^8.0.2",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"lint-staged": "^13.1.0",
		"release-it": "^15.6.0",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"tsup": "^6.5.0",
		"typescript": "^4.9.4"
	}
}
