{
	"name": "@morev/utils",
	"description": "Collection of JavaScript / TypeScript utilities",
	"type": "module",
	"version": "2.3.0",
	"private": false,
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"author": {
		"name": "Maxim Morev",
		"email": "max.seainside@gmail.com"
	},
	"homepage": "https://github.com/MorevM/utils#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/MorevM/utils"
	},
	"keywords": [
		"javascript",
		"typescript",
		"js",
		"ts",
		"utils",
		"utilities"
	],
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"prepare": "lefthook install",
		"vscode-settings": "cp .vscode/settings.template.json .vscode/settings.json",
		"build": "tsup",
		"test": "jest",
		"test:dev": "jest --watch",
		"lint": "eslint \"**/*.{js,ts,json,json5,jsonc,yml,yaml,md}\"",
		"lint:fix": "eslint \"**/*.{js,ts,json,json5,jsonc,yml,yaml,md}\" --fix",
		"release": "release-it --disable-metrics"
	},
	"dependencies": {
		"fast-copy": "^3.0.1",
		"fast-equals": "^5.0.1",
		"ohash": "^1.1.2",
		"type-fest": "^3.12.0"
	},
	"devDependencies": {
		"@morev/commitlint-config": "^0.2.2",
		"@morev/eslint-config": "^24.2.2",
		"@release-it/conventional-changelog": "^7.0.0",
		"@types/jest": "^29.5.3",
		"eslint": "^8.44.0",
		"jest": "^29.6.1",
		"jest-environment-jsdom": "^29.6.1",
		"lefthook": "^1.4.5",
		"release-it": "^16.1.0",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"tsup": "^7.1.0",
		"typescript": "^5.1.6"
	}
}
